{
  "name": "rvdlee/zf-image-optimiser",
  "description": "A package that allows you optimise images. To reduce in size with very slight quality loss.",
  "keywords": [
    "zf3",
    "image optimizer",
    "pngquant",
    "zend framework 3"
  ],
  "homepage": "https://thoughtforcoding.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Rob van der Lee",
      "email": "epicsoftworks@gmail.com",
      "homepage": "https://thoughtforcoding.com/"
    }
  ],
  "require": {
    "php": "^7.2 || ^7.3",
    "zendframework/zend-modulemanager": "^2.8",
    "zendframework/zend-servicemanager": "^2.7.8 || ^3.3",
    "zendframework/zend-filter": "^2.9",
    "zendframework/zend-validator": "*",
    "zendframework/zend-stdlib": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Rvdlee\\ZfImageOptimizer\\": "src/"
    },
    "classmap": [
      "src/Module.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Rvdlee\\ZfImageOptimizer\\Test\\": "tests"
    }
  }
}